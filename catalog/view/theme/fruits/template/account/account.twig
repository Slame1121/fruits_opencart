{{ header }}
<section class="breadcrumbs_block">
    <div class="container">
        <div class="breadcrumbs_block_container">
            <ul class="breadcrumbs_block_container__bread-crumbs">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
</section>
<section class="profile_data">
    <div class="container">
            {{ profile_edit }}
        <div class="profile_data_tabs">
            <div class="tabs_container profile_data_tabs-container">
                <ul class="tabs_list profile_data_tabs-container__list">
                    <li ><a class="active" href="#tab_wishes" >Избранное</a></li>
                    <li><a href="#tab_orders">История заказов</a></li>
                </ul>
                <div class="tabs_items profile_data_tabs-container__items">
                    <div id="tab_wishes" class=" profile_data_tabs-container__item active">
                        {{ wishlist }}
                    </div>
                    <div id="tab_orders" class=" profile_data_tabs-container__item">
                        {{ orders }}
                    </div>
                </div>
            </div>
        </div>

    </div>

</section>
{{ footer }}